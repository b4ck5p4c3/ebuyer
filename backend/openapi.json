                                    "$ref": "#/components/schemas/ErrorApiResponse"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "cookie": []
                    }
                ],
                "summary": "Get self member auth info",
                "tags": [
                    "auth"
                ]
            }
        }
    },
    "info": {
        "title": "EBuyer",
        "description": "Buylist application",
        "version": "1.0",
        "contact": {}
    },
    "tags": [],
    "servers": [],
    "components": {
        "securitySchemes": {
            "cookie": {
                "type": "apiKey",
                "in": "cookie",
                "name": "session"
            }
        },
        "schemas": {
            "ShopDTO": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string",
                        "format": "uuid"
                    },
                    "internalId": {
                        "type": "string"
                    },
                    "name": {
                        "type": "string"
                    }
                },
                "required": [
                    "id",
                    "internalId",
                    "name"
                ]
            },
            "ErrorApiResponse": {
                "type": "object",
                "properties": {
                    "statusCode": {
                        "type": "number"
                    },
                    "message": {
                        "type": "string"
                    }
                },
                "required": [
                    "statusCode",
                    "message"
                ]
            },
            "ItemDTO": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string",
                        "format": "uuid"
                    },
                    "title": {
                        "type": "string"
                    },
                    "comment": {
                        "type": "string"
                    },
                    "sku": {
                        "type": "string"
                    },
                    "imageUrl": {
                        "type": "string"
                    },
                    "itemUrl": {
                        "type": "string"
                    },
                    "price": {
                        "type": "string"
                    },
                    "requiredCount": {
                        "type": "number"
                    },
                    "boughtCount": {
                        "type": "number"
                    },
                    "status": {
                        "type": "string",
                        "enum": [
                            "created",
                            "canceled",
                            "fulfilled"
                        ]
                    },
                    "additionalData": {
                        "type": "object"
                    }
                },
                "required": [
                    "id",
                    "title",
                    "requiredCount",
                    "boughtCount",
                    "status"
                ]
            },
            "CreateItemDTO": {
                "type": "object",
                "properties": {
                    "shopId": {
                        "type": "string",
                        "format": "uuid"
                    },
                    "title": {
                        "type": "string"
                    },
                    "comment": {
                        "type": "string"
                    },
                    "sku": {
                        "type": "string"
                    },
                    "imageUrl": {
                        "type": "string"
                    },
                    "itemUrl": {
                        "type": "string"
                    },
                    "price": {
                        "type": "string"
                    },
                    "requiredCount": {
                        "type": "number"
                    },
                    "additionalData": {
                        "type": "object"
                    }
                },
                "required": [
                    "shopId",
                    "title",
                    "requiredCount"
                ]
            },
            "BuyItemDTO": {
                "type": "object",
                "properties": {
                    "additionalBoughtCount": {
                        "type": "number"
                    }
                },
                "required": [
                    "additionalBoughtCount"
                ]
            },
            "SelfAuthInfoDTO": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string",
                        "format": "uuid"
                    }
                },
                "required": [
                    "id"
                ]
            }
        }
    }
}