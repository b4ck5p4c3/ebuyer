{
    "openapi": "3.0.0",
    "paths": {
        "/api/shops": {
            "get": {
                "operationId": "ShopsController_findAll",
                "parameters": [],
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/ShopDTO"
                                    }
                                }
                            }
                        }
                    },
                    "default": {
                        "description": "Erroneous response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ErrorApiResponse"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "cookie": []
                    }
                ],
                "summary": "Get all shops",
                "tags": [
                    "shops"
                ]
            }
        },
        "/api/items/shop/{shopId}": {
            "get": {
                "description": "Get all unfulfilled items by shop",
                "operationId": "ItemsController_findAllUnfulfilledByShop",
                "parameters": [
                    {
                        "name": "shopId",
                        "required": true,
                        "in": "path",
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/ItemDTO"
                                    }
                                }
                            }
                        }
                    },
                    "default": {
                        "description": "Erroneous response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ErrorApiResponse"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "cookie": []
                    }
                ],
                "summary": "",
                "tags": [
                    "items"
                ]
            }
        },
        "/api/items": {
            "post": {
                "description": "Create item",
                "operationId": "ItemsController_create",
                "parameters": [],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/CreateItemDTO"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ItemDTO"
                                }
                            }
                        }
                    },
                    "default": {
                        "description": "Erroneous response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ErrorApiResponse"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "cookie": []
                    }
                ],
                "summary": "",
                "tags": [
                    "items"
                ]
            }
        },
        "/api/items/{itemId}/buy": {
            "patch": {
                "description": "Update item bought counter",
                "operationId": "ItemsController_buyItem",
                "parameters": [
                    {
                        "name": "itemId",
                        "required": true,
                        "in": "path",
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/BuyItemDTO"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ItemDTO"
                                }
                            }
                        }
                    },
                    "default": {
                        "description": "Erroneous response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ErrorApiResponse"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "cookie": []
                    }
                ],
                "summary": "",
                "tags": [
                    "items"
                ]
            }
        },
        "/api/items/{itemId}/cancel": {
            "patch": {
                "description": "Cancel item",
                "operationId": "ItemsController_cancelItem",
                "parameters": [
                    {
                        "name": "itemId",
                        "required": true,
                        "in": "path",
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ItemDTO"
                                }
                            }
                        }
                    },
                    "default": {
                        "description": "Erroneous response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ErrorApiResponse"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "cookie": []
                    }
                ],
                "summary": "",
                "tags": [
                    "items"
                ]
            }
        },
        "/api/auth/self": {
            "get": {
                "operationId": "AuthController_getSelf",
                "parameters": [],
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/SelfAuthInfoDTO"
                                }
                            }
                        }
                    },
                    "default": {
                        "description": "Erroneous response",
                        "content": {
